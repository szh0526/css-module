{
  "name": "css-modules-demos",
  "version": "1.0.0",
  "description": "A collection of simple demos of CSS Modules",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build/*",
    "mock": "export DEBUG_WITH_MOCK=1 && npm start",
    "debug_mock": "export DEBUG_WITH_MOCK=1 && devtool app.js --watch",
    "debug": "devtool app.js --watch",
    "pm2": "pm2 start pm2.json --env test",
    "start": "node app",
    "devserver": "webpack-dev-server --config webpack.config.js --watch --hot --inline --cache",
    "server": "http-server -p 10000 dist/",
    "lines": "find static -name \"*.js\" | xargs cat | wc -l",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "CSS",
    "CSS Modules"
  ],
  "author": "Ruan Yifeng",
  "license": "ISC",
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "gulp": "^3.9.1",
    "gulp-postcss": "^7.0.0",
    "gulp-sass": "^3.1.0",
    "postcss-loader": "^2.0.9",
    "postcss-modules-values": "^1.1.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
